{
  "name": "Diff Developer Tools",
  "description": "Adds a local version of diff",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "60",
  "devtools_page": "devtools.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "main.html",
    "panel.html",
    "build/backend.js",
    "http://localhost:9000/*",
    "https://api.getdiff.app/*",
    "https://s3.amazonaws.com/getdiff-static-client/*"
  ],
  "permissions": [
    "file:///*",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}